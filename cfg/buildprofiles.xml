<?xml version="1.0" encoding="UTF-8"?>
<buildprofiles>
    <variables>
        <header-logo>kotlin-multiplatform-logo.svg</header-logo>
        <primary-color>frozen</primary-color>
        <browser-edits-url>https://github.com/JetBrains/kotlin-multiplatform-dev-docs/edit/master/docs/</browser-edits-url>
        <web-root>https://www.jetbrains.com/help</web-root>
        <web-community-path>Developer Community:https://www.jetbrains.net/devnet/community/idea/kb</web-community-path>
        <download-page/>
        <fallback-url>https://www.jetbrains.com/help/kotlin-multiplatform-dev/</fallback-url>
        <config-feedbackRequireEmail>false</config-feedbackRequireEmail>
<!--        <config-showDownloadButton>true</config-showDownloadButton>-->
        <config-disqus-id>jetbrains</config-disqus-id>
        <config-disqus-show>false</config-disqus-show>
        <config-feedback-url>https://forms-service.jetbrains.com/feedback</config-feedback-url>
        <config-feedback-enabled>true</config-feedback-enabled>
        <config-feedback-widget>true</config-feedback-widget>
<!--        <config-webmaster>webmaster@jetbrains.com</config-webmaster>-->
<!--        <config-feedback-support>https://kotl.in/issue</config-feedback-support>-->
        <config-sideblocks>true</config-sideblocks>
        <config-search-scopes-provider>https://data.services.jetbrains.com/search/</config-search-scopes-provider>
        <og-twitter>@kotlin</og-twitter>
        <og-image>https://resources.jetbrains.com/storage/products/jetbrains/img/meta/preview.png</og-image>
        <property-cleanup>[:,.;]$|\.{3}$|[^a-zA-Z0-9}')]+$</property-cleanup>
    </variables>
    <footer>
        <social type="twitter" href="https://twitter.com/kotlin">X (former twitter)</social>
        <social type="blog" href="https://blog.jetbrains.com/kotlin/category/multiplatform/">Kotlin Multiplatform blog</social>
        <social type="youtube" href="https://www.youtube.com/c/kotlin">Kotlin YouTube channel</social>
        <link href="https://kotl.in/gamt3a">Kotlin Multiplatform Slack</link>
    </footer>
    <build-profile product="mpd"/>
</buildprofiles>

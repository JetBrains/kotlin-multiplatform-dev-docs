<?xml version="1.0" encoding="UTF-8"?>
<buildprofiles xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://resources.jetbrains.com/writerside/1.0/build-profiles.xsd">
	<sitemap priority="0.35" change-frequency="monthly"/>
	<variables>
		# newly added tags
		<analytics-head-script-file>thirdparty/dpk-gtm-analytics.html</analytics-head-script-file>
		<include-in-head>thirdparty/favicons.html</include-in-head>
		<website-title>%instance% Documentation</website-title>
		<automaticLinkSummaries>false</automaticLinkSummaries>
		# old tags
		<header-logo>kotlin-multiplatform-logo.svg</header-logo>
		<product-web-url>https://www.jetbrains.com/kotlin-multiplatform/</product-web-url>
		<primary-color>frozen</primary-color>
		<enable-browser-edits>true</enable-browser-edits>
		<browser-edits-url>https://github.com/JetBrains/kotlin-multiplatform-dev-docs/edit/master/</browser-edits-url>
		<web-root>https://www.jetbrains.com/help</web-root>
		<web-community-path>Developer Community:https://www.jetbrains.net/devnet/community/idea/kb</web-community-path>
		<download-page/>
		<fallback-url>https://www.jetbrains.com/help/kotlin-multiplatform-dev/</fallback-url>
		<feedbackRequireEmail>false</feedbackRequireEmail>
		<!--        <showDownloadButton>true</showDownloadButton>-->
		<disqus-id>jetbrains</disqus-id>
		<disqus-show>false</disqus-show>
		<feedback-url>https://forms-service.jetbrains.com/feedback</feedback-url>
		<feedback-enabled>true</feedback-enabled>
		<feedback-widget>true</feedback-widget>
		<!--        <webmaster>webmaster@jetbrains.com</webmaster>-->
		<!--        <feedback-support>https://kotl.in/issue</feedback-support>-->
		<sideblocks>true</sideblocks>
		<search-scopes-provider>https://data.services.jetbrains.com/search/</search-scopes-provider>
		<!--        <versions-switcher>https://data.services.jetbrains.com/help-versions</versions-switcher>-->
		<og-twitter>@kotlin</og-twitter>
		<og-image>https://resources.jetbrains.com/storage/products/jetbrains/img/meta/preview.png</og-image>
		<noindex-content-in-deprecated>false</noindex-content-in-deprecated>
		<noindex-content-in-eap>false</noindex-content-in-eap>
		<noindex-content-in-release>false</noindex-content-in-release>
		<property-cleanup>[:,.;]$|\.{3}$|[^a-zA-Z0-9}')]+$</property-cleanup>
		<generate-canonicals>true</generate-canonicals>
	</variables>
	<footer>
		<link href="https://github.com/JetBrains/compose-multiplatform">GitHub</link>
		<social type="twitter" href="https://twitter.com/kotlin">X (former Twitter)</social>
		<social type="blog" href="https://blog.jetbrains.com/kotlin/category/multiplatform/">Kotlin Multiplatform blog</social>
		<social type="youtube" href="https://www.youtube.com/c/kotlin">Kotlin YouTube channel</social>
		<!-- Custom CSS will replace rss type -->
		<social type="rss" href="https://kotl.in/gamt3a">Kotlin Multiplatform Slack</social>
	</footer>
	<build-profile instance="mpd"/>
</buildprofiles>